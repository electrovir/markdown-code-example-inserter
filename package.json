{
    "name": "markdown-code-example-inserter",
    "version": "0.3.0",
    "description": "Syncs code examples with markdown documentation.",
    "keywords": [
        "automatic",
        "code",
        "examples",
        "insert",
        "inserter",
        "interpolator",
        "markdown",
        "readme",
        "snippet"
    ],
    "homepage": "https://github.com/electrovir/markdown-code-example-inserter",
    "bugs": {
        "url": "https://github.com/electrovir/markdown-code-example-inserter/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/electrovir/markdown-code-example-inserter"
    },
    "license": "MIT",
    "author": {
        "name": "electrovir",
        "url": "https://github.com/electrovir"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": {
        "md-code": "dist/cli.js"
    },
    "scripts": {
        "compile": "virmator compile",
        "docs:update": "virmator code-in-markdown",
        "format": "virmator format",
        "publish": "virmator publish \"npm run test:all\"",
        "test": "npm run compile && virmator test",
        "test:all": "npm run test:types && npm run test:coverage && npm run test:spelling && npm run test:format",
        "test:coverage": "npm run test coverage",
        "test:docs": "virmator code-in-markdown check",
        "test:format": "virmator format check",
        "test:spelling": "virmator spellcheck",
        "test:types": "tsc --noEmit"
    },
    "dependencies": {
        "@augment-vir/common": "^12.6.0",
        "@augment-vir/node-js": "^12.6.0",
        "fs-extra": "11.1.0",
        "glob-promise": "4.2.2",
        "language-map": "1.5.0",
        "rehype-parse": "7.0.1",
        "remark-parse": "9.0.0",
        "remark-rehype": "8.1.0",
        "typescript": "4.9.5",
        "unified": "9.2.2"
    },
    "devDependencies": {
        "@electrovir/nyc": "^15.1.0-fix0",
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@types/chai": "^4.3.4",
        "@types/chai-as-promised": "^7.1.5",
        "@types/fs-extra": "11.0.1",
        "@types/mocha": "^10.0.1",
        "@types/node": "18.13.0",
        "chai": "^4.3.7",
        "chai-as-promised": "^7.1.1",
        "cspell": "^6.25.0",
        "istanbul-smart-text-reporter": "^1.1.1",
        "mocha": "^10.2.0",
        "mocha-spec-reporter-with-file-names": "^0.0.3",
        "prettier": "^2.8.4",
        "prettier-plugin-jsdoc": "^0.4.2",
        "prettier-plugin-multiline-arrays": "^1.1.3",
        "prettier-plugin-organize-imports": "^3.2.2",
        "prettier-plugin-packagejson": "^2.4.3",
        "prettier-plugin-sort-json": "^1.0.0",
        "prettier-plugin-toml": "^0.3.1",
        "test-established-expectations": "^0.2.0",
        "ts-node": "^10.9.1",
        "virmator": "5.4.0"
    }
}
