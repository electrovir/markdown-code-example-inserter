{
    "name": "markdown-code-example-inserter",
    "version": "0.3.3",
    "description": "Syncs code examples with markdown documentation.",
    "keywords": [
        "automatic",
        "code",
        "examples",
        "insert",
        "inserter",
        "interpolator",
        "markdown",
        "readme",
        "snippet"
    ],
    "homepage": "https://github.com/electrovir/markdown-code-example-inserter",
    "bugs": {
        "url": "https://github.com/electrovir/markdown-code-example-inserter/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/electrovir/markdown-code-example-inserter"
    },
    "license": "MIT",
    "author": {
        "name": "electrovir",
        "url": "https://github.com/electrovir"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": {
        "md-code": "dist/cli.js"
    },
    "scripts": {
        "compile": "virmator compile",
        "docs:update": "virmator docs",
        "format": "virmator format",
        "publish": "virmator publish \"npm run test:all\"",
        "test": "npm run compile && virmator test",
        "test:all": "npm run compile && concurrently --colors --kill-others-on-fail -c auto --names types,tests,spelling,format,deps \"npm run test:types\" \"npm run test:coverage\" \"npm run test:spelling\" \"npm run test:format\" \"npm run test:deps\"",
        "test:coverage": "npm run test coverage",
        "test:deps": "virmator deps check",
        "test:docs": "virmator docs check",
        "test:format": "virmator format check",
        "test:spelling": "virmator spellcheck",
        "test:types": "tsc --noEmit"
    },
    "dependencies": {
        "@augment-vir/common": "^22.0.0",
        "@augment-vir/node-js": "^22.0.0",
        "fs-extra": "11.1.1",
        "glob-promise": "6.0.5",
        "language-map": "1.5.0",
        "rehype-parse": "7.0.1",
        "remark-parse": "9.0.0",
        "remark-rehype": "8.1.0",
        "typescript": "5.2.2",
        "unified": "9.2.2"
    },
    "devDependencies": {
        "@electrovir/nyc": "^15.1.0-fix0",
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@types/chai": "^4.3.10",
        "@types/chai-as-promised": "^7.1.8",
        "@types/fs-extra": "11.0.4",
        "@types/hast": "2.3.8",
        "@types/mdast": "3.0.15",
        "@types/mocha": "^10.0.4",
        "@types/node": "20.9.2",
        "@types/unist": "2.0.10",
        "chai": "^4.3.10",
        "chai-as-promised": "^7.1.1",
        "cspell": "^8.0.0",
        "dependency-cruiser": "^15.3.0",
        "esbuild": "^0.19.6",
        "istanbul-smart-text-reporter": "^1.1.3",
        "mocha": "^10.2.0",
        "mocha-spec-reporter-with-file-names": "^0.0.3",
        "npm-check-updates": "~16.12.3",
        "prettier": "^3.1.0",
        "prettier-plugin-interpolated-html-tags": "^1.0.2",
        "prettier-plugin-jsdoc": "^1.1.1",
        "prettier-plugin-multiline-arrays": "^3.0.0",
        "prettier-plugin-organize-imports": "^3.2.4",
        "prettier-plugin-packagejson": "^2.4.6",
        "prettier-plugin-sort-json": "^3.1.0",
        "prettier-plugin-toml": "^1.0.0",
        "test-established-expectations": "^0.5.1",
        "ts-node": "^10.9.1",
        "virmator": "11.1.2"
    }
}
