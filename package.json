{
    "name": "markdown-code-example-inserter",
    "version": "0.1.12",
    "description": "Syncs code examples with markdown documentation.",
    "keywords": [
        "automatic",
        "code",
        "examples",
        "insert",
        "inserter",
        "interpolator",
        "markdown",
        "readme",
        "snippet"
    ],
    "homepage": "https://github.com/electrovir/markdown-code-example-inserter",
    "bugs": {
        "url": "https://github.com/electrovir/markdown-code-example-inserter/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/electrovir/markdown-code-example-inserter"
    },
    "license": "MIT",
    "author": {
        "name": "electrovir",
        "url": "https://github.com/electrovir"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": {
        "md-code": "dist/cli.js"
    },
    "scripts": {
        "build": "tsc",
        "compile": "rm -rf dist && virmator compile",
        "docs:update": "virmator code-in-markdown",
        "format": "virmator format",
        "prepublishOnly": "npm run test:all && npm run build",
        "preview": "virmator frontend preview",
        "spellcheck": "virmator spellcheck",
        "start": "npm install && virmator frontend",
        "test": "npm run build && virmator test",
        "test:all": "npm run test:types && npm test && npm run spellcheck && npm run test:format",
        "test:format": "virmator format check",
        "test:types": "tsc --noEmit"
    },
    "dependencies": {
        "@augment-vir/common": "^6.1.5",
        "@augment-vir/node-js": "^6.1.5",
        "fs-extra": "11.1.0",
        "glob-promise": "5.0.0",
        "language-map": "1.5.0",
        "rehype-parse": "7.0.1",
        "remark-parse": "9.0.0",
        "remark-rehype": "8.1.0",
        "typescript": "4.9.4",
        "unified": "9.2.2"
    },
    "devDependencies": {
        "@electrovir/nyc": "^15.1.0",
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@types/chai": "^4.3.4",
        "@types/chai-as-promised": "^7.1.5",
        "@types/fs-extra": "9.0.13",
        "@types/mocha": "^10.0.1",
        "@types/node": "18.11.12",
        "chai": "^4.3.7",
        "chai-as-promised": "^7.1.1",
        "cspell": "^6.17.0",
        "istanbul-smart-text-reporter": "^1.0.1",
        "mocha": "^10.1.0",
        "mocha-spec-reporter-with-file-names": "^0.0.2",
        "prettier": "^2.8.1",
        "prettier-plugin-jsdoc": "^0.4.2",
        "prettier-plugin-multiline-arrays": "^1.1.2",
        "prettier-plugin-organize-imports": "^3.2.1",
        "prettier-plugin-packagejson": "^2.3.0",
        "prettier-plugin-sort-json": "^0.0.3",
        "prettier-plugin-toml": "^0.3.1",
        "ts-node": "^10.9.1",
        "virmator": "5.0.6"
    }
}
